000100 00001 00000 0000	0000 0000 1101	// beq r1,r0,exit1
000100 00010 00000 0000 0000 0001 0001	// beq r2,r0,exit2

000000 00001 00010 00100 00000 100010 //sub r4,r1,r2

000111 00100 00000 0000 0000 0000 0110 // bgtz r4,a1

00000 00000 00000 00000 00000 00000 00000 00000 //no-op

00000 00000 00000 00000 00000 00000 00000 00000 //no-op

000000 00010 00001 00010 00000 100010 //sub r2,r2,r1

000010 0000 0000 0000 0000 0000 0000 10 // j loop

